{
  "expo": {
    "name": "Orpheus",
    "slug": "Orpheus",
    "scheme": "orpheus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo-small.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/logo-small.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPList": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "config": {
        "useNonExemptEncryption": false
      },
      "bundleIdentifier": "com.tanmayep.orpheus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/logo-small.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tanmayep.orpheus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
